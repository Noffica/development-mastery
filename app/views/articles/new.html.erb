<h1>Create a new article here</h1>

<%= form_with model: @article_new do |form| %>
  <div>
    <%= form.label(:title) %>
    <br>
    <%= form.text_field(:title) %>
  </div>

  <div>
    <%= form.label(:body) %>
    <br>
    <%= form.text_area(:body) %>
  </div>

  <div>
    <%= form.submit %>
  </div>
<% end %>
